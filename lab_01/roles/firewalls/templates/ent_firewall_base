!
interface GigabitEthernet0/0
 nameif OUTSIDE
 security-level 100
 ip address 10.0.0.2 255.255.255.25
 !
interface GigabitEthernet0/2
 nameif INSIDE
 security-level 100
 ip address 10.1.0.33 255.255.255.252
 !
interface GigabitEthernet0/1.99
 vlan 99
 nameif DMZ_MGMT
 security-level 0
 ip address 10.1.0.1 255.255.255.240
 !
route OUTSIDE 0.0.0.0 0.0.0.0 10.0.0.1 1
!
object-group network INSIDE_NETS
 network-object 172.16.0.0 255.255.255.0
